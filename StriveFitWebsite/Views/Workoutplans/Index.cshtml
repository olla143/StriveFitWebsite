@model IEnumerable<StriveFitWebsite.Models.Workoutplan>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<div class="content">
    <h1>Index</h1>

    <p>
        <a asp-action="Create">Create New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Member)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Schedule)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Trainer)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
    @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Member.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Schedule.Scheduleid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Trainer.Name)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Workoutid">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Workoutid">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Workoutid">Delete</a>
                </td>
            </tr>
    }
        </tbody>
    </table>
</div>