@model StriveFitWebsite.Models.Workoutplan

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<div class="content">
    <h1>Details</h1>

    <div>
        <h4>Workoutplan</h4>
        <hr />
        <dl class="row">
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Member)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Member.Name)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Schedule)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Schedule.Scheduleid)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Trainer)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Trainer.Name)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model?.Workoutid">Edit</a> |
        <a asp-action="Index">Back to List</a>
    </div>
</div>