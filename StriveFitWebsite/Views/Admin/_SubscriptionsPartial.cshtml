@model List<StriveFitWebsite.Models.Subscription>

<table class="table text-start align-middle table-bordered table-hover mb-0">
    <thead>
        <tr class="text-white">
            <th scope="col">User</th>
            <th scope="col">Plan</th>
            <th scope="col">Renewal Status</th>
            <th scope="col">Payment Status</th>
            <th scope="col">Start Date</th>
            <th scope="col">End Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var subscription in Model)
        {
            <tr>
                <td>@subscription.User.Name</td>
                <td>@subscription.Plan.Planname</td>
                <td>@subscription.Renewalstatus</td>
                <td>@subscription.Paymentstatus</td>
                <td>@subscription.Startdate.ToString("dd MMM yyyy")</td>
                <td>@subscription.Enddate.ToString("dd MMM yyyy")</td>
            </tr>
        }
    </tbody>
</table>
