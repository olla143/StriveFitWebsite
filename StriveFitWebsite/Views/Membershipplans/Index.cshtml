@model IEnumerable<StriveFitWebsite.Models.Membershipplan>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";

}
<div class="content">
    <h1>Index</h1>

    <p>
        <a asp-action="Create">Create New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Planname)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Price)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Durationmonths)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Planname)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Price)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Durationmonths)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.Planid">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.Planid">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.Planid">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>